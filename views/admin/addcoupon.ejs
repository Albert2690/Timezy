    <script>

        function generatecoupon(){
            $.ajax({
                url:"/admin/generatecoupon",
                method:"get",
                success:((response)=>{
                   
                    // console.log((response.couponCode));
                    document.getElementById("couponcode").value=response.couponCode

                })
            })
        }

        function validateform(){
            event.preventDefault();
           
        const couponName = document.getElementById('couponcode')
        const validity = document.getElementById('validity')
        const minAmount = document.getElementById('minamount')
        const discountPercentage = document.getElementById('discount')
        const maxDiscount = document.getElementById('maxdiscountamount')
      

        const couponNameValue = couponName.value.trim()
        const validityValue = validity.value.trim()
        const minAmountValue = minAmount.value.trim()
        const discountPercentageValue = discountPercentage.value.trim()
        const maxDiscountValue = maxDiscount.value.trim()
       

        const error1 = couponName.parentElement.querySelector('p')
        const error2 = validity.parentElement.querySelector('p')
        const error3 = minAmount.parentElement.querySelector('p')
        const error4 = discountPercentage.parentElement.querySelector('p')
        const error5 = maxDiscount.parentElement.querySelector('p')
       

        error1.innerText = ''
        error2.innerText = ''
        error3.innerText = ''
        error4.innerText = ''
        error5.innerText = ''
       

        let flag = 1

        if (couponNameValue == '') {
            error1.innerText = 'Coupon code is required'
            flag = 2
        }
        if (validityValue == '') {
            error2.innerText = 'Validity is required'
            flag = 2
        }
        if (minAmountValue == '' || minAmountValue<0) {
            error3.innerText = 'Minimum amount is required'
            flag = 2
        }
        if (discountPercentageValue == '' ||  discountPercentageValue<0) {
            error4.innerText = 'Discount percentage is required and should be a positive value'
            flag = 2
        }
        if (maxDiscountValue == ''||  maxDiscountValue<0) {
            error5.innerText = 'Maximum discount is required and should be a positive value'
            flag = 2
        }
        

        if (flag == 1) {
            console.log("helllllooi");
            console.log(couponNameValue);
            console.log(validityValue);

            console.log(minAmountValue);

            console.log(discountPercentageValue);

            console.log(maxDiscountValue);

            submitfunction(couponNameValue, validityValue, minAmountValue, discountPercentageValue, maxDiscountValue)
        }


    }


        function submitfunction (couponcode,validity,minanmount,maxdiscount,discount){
            console.log("hiiiioo");
            console.log(couponcode);
            console.log(validity);

            console.log(minanmount);
            console.log(maxdiscount);
            console.log(discount);

            $.ajax({
                url:"/admin/addcoupon",
                method:'post',
                data:{
                    couponcode:couponcode,validity:validity,minamount:minanmount,maxdiscount:maxdiscount,discount:discount
                },
                success:((response)=>{
                    if(response.status===true){
                        Swal.fire({
                            title:"Coupon added successfully",
                            icon:"success",
                            showConfirmButton:false,
                            timer:2000
                           
                        }) .then(()=>{
                                location.reload()
                            })
                    }
                })
            })
        }
        

    </script>

