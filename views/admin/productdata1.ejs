        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products List</h2>
                   
                </div>
                <div>
                    <!-- <a href="#" class="btn btn-light rounded font-md">Export</a>
                    <a href="#" class="btn btn-light rounded  font-md">Import</a> -->
                    <a href="/admin/insertproduct" class="btn btn-primary btn-sm rounded">Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row align-items-center">
                       
                       
                        <!-- <div class="col-md-2 col-6">
                            <input type="date" value="02.05.2021" class="form-control">
                        </div> -->
                        <div class="card">
                         
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th scope="col" class="col-check">Check</th>
                                                <th scope="col" class="col-name">Name</th>
                                                
                                                <th scope="col" class="col-name">category</th>

                                                <th scope="col" class="col-price">Price</th>
                                                <th scope="col" class="col-price">Offer Percentage</th>
                                                <th scope="col" class="col-price">Offer validity</th>

                                                <th scope="col" class="col-status">Status</th>
                                                <th scope="col" class="col-action text-end">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% product.forEach((item) => { %>
                                            <tr>
                                                <td class="col-check">
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox">
                                                    </div>
                                                </td>
                                                <td class="col-name">
                                                    <a class="itemside" href="#">
                                                        <div class="left">
                                                            <img src="../product-images/<%=item.image[0]%>" class="img-sm img-thumbnail" alt="Item" style="width: 40px;height: 35px;" >
                                                        </div>
                                                        <div class="info">
                                                            <h6 class="mb-0"><%=item.name%></h6>
                                                        </div>
                                                       
                                                    </a>
                                                </td>
                                                <td> <div class="info">
                                                    <h6 class="mb-0"><%=item.category.name%></h6>
                                                </div></td>
                                                <td class="col-price"><span><%=item.price%></span></td>
                                              <%  if(item.discountpercentage){%>

                                                
                                                <td class="col-price"><span><%=item.discountpercentage%></span></td>
                                                  
                                                <td class="col-price"><span><%=item.offervalidity.toLocaleDateString()%></span></td>
                                                <%}else{%>
                                                    <td class="col-price"><span>...</span></td>
                                                <td class="col-price"><span>...</span></td>

                                                <%}%>
                                                <td class="col-status">
                                                    <% if (item.is_listed == 0) { %>
                                                    <span class="badge rounded-pill alert-success">Listed</span>
                                                    <% } else { %>
                                                    <span class="badge rounded-pill alert-danger">Unlist</span>
                                                    <% } %>
                                                </td>
                                                <td class="col-action text-end">
                                                    <div class="btn-group">
                                                        <a href="/admin/editproduct?id=<%=item._id%>" class="btn btn-sm font-sm rounded btn-brand">
                                                            <i class="material-icons md-edit"></i> Edit
                                                        </a>
                                                        <a href="/admin/unlistproduct?id=<%=item._id%>" class="btn btn-sm font-sm btn-light rounded">
                                                            <i class="material-icons md"></i> UnList
                                                        </a>
                                                    </div>
                                                </td>
                                            </tr>
                                            <% }) %>
                                        </tbody>
                                    </table>
                                </div>
                            </div> <!-- card-body end// -->
                        </div> <!-- card end// -->
                        

                   
