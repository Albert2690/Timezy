                                            <a href="#" data-bs-toggle="dropdown" class="btn btn-light rounded btn-sm font-sm"> <i class="material-icons md-more_horiz"></i> </a>
                                            <div class="dropdown-menu">
                                                <a class="dropdown-item" onclick="returnstatus('<%=items._id%>','return accepted')">Accept</a>
                                               
                                                <a class="dropdown-item text-danger" onclick="returnstatus('<%=items._id%>','return declined')">Decline</a>
                                            </div>
                                        <%}else if(items.status==='return accepted'){%>
                                            <a href="#" data-bs-toggle="dropdown" class="btn btn-light rounded btn-sm font-sm"> <i class="material-icons md-more_horiz"></i> </a>
                                            <div class="dropdown-menu">
                                                <a class="dropdown-item" onclick="orderstatus('<%=items._id%>','refunded')">Refund Amount</a>
                                                </div>
                                       <% }else{%>
                                        <td>...</td>
                                       <%}%>
                                        </div> <!-- dropdown //end -->

                                       
                                    </td>
                                    <td> <a href="#" class="btn btn-md rounded font-sm">Detail</a></td>
                            
                           
                                </tr>
                                <% })%>
                               
